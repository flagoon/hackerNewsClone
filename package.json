{
  "name": "hackernewsclone",
  "version": "1.0.0",
  "description": "Simple Hackernews clone",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:prod": "rm -rf lib && webpack --config webpack.config.prod.js",
    "build:hot": "yarn build:dev --hot",
    "build:dev": "rm -rf dist && webpack-dev-server --config webpack.config.dev.js",
    "format": "pretty-quick",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flagoon/hackerNewsClone.git"
  },
  "author": "Pawel Kochanek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flagoon/hackerNewsClone/issues"
  },
  "homepage": "https://github.com/flagoon/hackerNewsClone#readme",
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "core-js": "^3.6.5",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-loader": "^4.0.2",
    "fork-ts-checker-notifier-webpack-plugin": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.7",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-hot-loader": "^4.12.21",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty-quick; yarn type-check"
    }
  }
}
